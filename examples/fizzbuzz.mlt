define divisible (Int, Int -> Bool) { (%) 0 (=) }

define fizzbuzz (Int -> String) {
  -> n;
  do (with (+Fail)) { n 5 divisible n 3 divisible }
  if { if { "FizzBuzz" } else { "Fizz" } }
  else { if { "Buzz" } else { n show } }
}

define fizzbuzzes (Int, Int ->) {
  -> c, m;
  c fizzbuzz println (c < m)
  if { (c + 1) m fizzbuzzes }
  else {}
}

1 100 fizzbuzzes
