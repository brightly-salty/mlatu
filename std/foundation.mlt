vocab mlatu {
  intrinsic call (∀ r... s... . r..., (r... -> s...) -> s...)
  intrinsic drop (∀ a . a ->)
  intrinsic swap (∀ a b . a, b -> b, a)
}

define call (∀ r... s... . r..., (r... -> s...) -> s...) {
  mlatu::call
}

define drop (∀ t . t ->) {
  mlatu::drop
}

define swap (∀ a! b! . a!, b! -> b!, a!) {
  mlatu::swap
}
