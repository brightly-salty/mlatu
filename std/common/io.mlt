data writable { stdout (-> writable) | stderr (-> writable) } 

define print (for t. t -> <io>) {
  show stdout write
}

define println (for t. t -> <io>) {
  show "\n" append stdout write
}

define read-line ( -> char list <io>) {
  "read-stdin" extern 
}

define write (char list, writable ->  <io>) {
  match
  | stdout {
    "write-stdout" extern
  }
  | stderr {
    "write-stderr" extern
  }
}
